{
  "expo": {
    "name": "AesthetIQ",
    "slug": "AesthetIQ",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aesthetiq",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.AesthetIQ",
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "Allow AesthetIQ to save photos to your photo library.",
        "NSPhotoLibraryUsageDescription": "Allow AesthetIQ to access your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_EXTERNAL_STORAGE"
      ],
      "package": "com.anonymous.AesthetIQ"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow AesthetIQ to access your camera to take photos."
        }
      ],
      "react-native-vision-camera",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
